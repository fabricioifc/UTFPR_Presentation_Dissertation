  \cmd@ifnotempty{\@logocampus}{\hskip10pt\@logocampus}%
  \cmd@ifnotempty{\@logomec}{\hskip10pt\@logomec}%
  \cmd@ifnotempty{\@logogov}{\hskip10pt\@logogov}%
}
